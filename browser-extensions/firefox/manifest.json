{

    "manifest_version": 2,
    "name": "hotreload",
    "version": "1.0",
  
    "description": "Firefox extension for hotreload.",
  
    "icons": {
      "48": "hotreload-48.png"
    },
  
    "content_scripts": [
      {
        "matches": ["*://*/*"],
        "js": ["hotreload.js"]
      }
    ]
  
  }